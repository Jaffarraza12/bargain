<body>

<!-- Coming Soon -->
<div class="coming-soon">
    <div class="inner-bg">
        <div class="container">
            <div class="row" style="min-height: 400px">
                <div class="col-sm-12">
                    <div class="logo wow fadeInDown">
                        <h1>
                            <a style="">Bargain.pk Coming Soon </a>
                        </h1>
                    </div>
                    <h2 class="wow fadeInLeftBig" style="color:#262626;font-weight:700">Coming Soon</h2>
                    <div class="timer wow fadeInUp" style="display:none;">
                        <div class="days-wrapper">
                            <span class="days"></span> <br><days></days>
                        </div>
                        <span class="slash">/</span>
                        <div class="hours-wrapper">
                            <span class="hours"></span> <br>hours
                        </div>
                        <span class="slash">/</span>
                        <div class="minutes-wrapper">
                            <span class="minutes"></span> <br>minutes
                        </div>
                        <span class="slash">/</span>
                        <div class="seconds-wrapper">
                            <span class="seconds"></span> <br>seconds
                        </div>
                    </div>
                    <div class="wow fadeInLeftBig">
                        <p >
                            <!--In the meantime sign up to our newsletter and you'll be one of the first to know when the site is ready:-->
                        </p>
                    </div>
                    <div class="subscribe wow fadeInUp" style="display: none">
                        <form class="form-inline" action="" method="post">
                            <input type="text" name="email" placeholder="Enter your email..." class="subscribe-email">
                            <button type="submit" class="btn">Subscribe</button>
                        </form>
                        <div class="success-message"></div>
                        <div class="error-message"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

