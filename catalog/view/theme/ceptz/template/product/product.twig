                                                    <label class="control-label" for="input-option{{ option.product_option_id }}">{{ option.name }}</label>
                                                    <div class="input-group time">
                                                        <input type="text" name="option[{{ option.product_option_id }}]" value="{{ option.value }}" data-date-format="HH:mm" id="input-option{{ option.product_option_id }}" class="form-control" />
                                                        <span class="input-group-btn">
                <button type="button" class="btn btn-default"><i class="fa fa-calendar"></i></button>
                </span></div>
                                                </div>
                                            {% endif %}
                                        {% endfor %}
                                    </div>

                                    <div class="options MDhidden  PADhidden">
                                            <a href="#option-html-content" class="open-popup-link mbl-option-popup"></a>
                                        <div class="mbl-option">
                                        {% for option in options %}

                                                <div class="option-tile ">
                                                    <h4 class="pull-left">{{ option.name }}:</h4>
                                                    <a class="pull-right"><icon class="fa fa-caret-right"></icon></a>
                                                    <div class="clearfix"></div>
                                                        <div class="option-tile-content">
                                                              <spana class="option_{{option.option_id}}">{{ option.product_option_value[0].name}}</spana>
                                                        </div>

                                                </div>

                                        {% endfor %}
                                        </div>
                                    </div>


                                    <div id="option-html-content" class="white-popup white-background mfp-hide">
                                        <div class="opt-html">
                                            <a class="pull-right closePopup opt-html-close ">DONE</a>
