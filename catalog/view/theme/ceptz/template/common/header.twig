<link rel="stylesheet" href="catalog/view/theme/ceptz/css/responsive.css" >
<link rel="stylesheet" type="text/css" href="catalog/view/javascript/jquery/magnific/magnific-popup.css" />
{% for style in styles %}
<link  href="{{ style.href }}" type="text/css" media="screen" rel="{{ style.rel }}" />
{% endfor %}
{% for link in links %}
<link rel="{{ link.rel }}" href="{{ link.href }}"  />
{% endfor %}
{% for analytic in analytics %}
{{ analytic }}
{% endfor %}
<script src="catalog/view/javascript/tag.js" type="text/javascript"></script>
