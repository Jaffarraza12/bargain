                                            <br/>
                                            <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5a6b19b8f1968205"></script>
                                            <div class="addthis_inline_share_toolbox"></div>
                                        </div>
                                        <div class="clearfix"></div>

                                    </div>


                                    {% if minimum > 1 %}
                                        <div class="alert alert-info"><i class="fa fa-info-circle"></i> {{ text_minimum }}</div>
                                    {% endif %}
                                </div>
                                <div class="custom_block">
                                    <div class="panel-group" id="accordion">
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <h4 class="panel-title"><a href="#question_1" onClick="return false;" data-toggle="collapse" data-parent="#accordion" class="accordion-toggle collapsed">How soon can I get this item?   <i class="fa fa-caret-down"></i></a></h4>
                                            </div>
                                            <div class="panel-collapse collapse" id="question_1" >
                                                <div class="panel-body">
                                                    {{ delivery_time }}
                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <h4 class="panel-title"><a href="#question_2" onClick="return false;" data-toggle="collapse" data-parent="#accordion" class="accordion-toggle collapsed">What are the payment options I have?  <i class="fa fa-caret-down"></i></a></h4>
                                            </div>
                                            <div class="panel-collapse collapse" id="question_2" >
                                                <div class="panel-body">
                                                    {{ payment_method }}
                                                </div>
                                            </div>
                                        </div>
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <h4 class="panel-title"><a href="#question_3" onClick="return false;"  data-toggle="collapse" data-parent="#accordion" class="accordion-toggle collapsed">What is your return & exchange policy?     <i class="fa fa-caret-down"></i></a></h4>
                                            </div>
                                            <div class="panel-collapse collapse" id="question_3">
                                                <div class="panel-body">
                                                   {{ return_policy }}
                                                    </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div><!-- End #product -->

                        </div>


                    </div>

                </div>
           </div>
</div>
